<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Style="{DynamicResource backgroundStyle}"
             x:Class="TweetTail.Pages.Blend.BlendListPage">
    <ContentPage.Content>
        <ListView x:Name="blendListView"
                  SelectionMode="None">
            <ListView.Header>
                <Label Style="{DynamicResource labelStyle}" x:Name="lblHeader" Text="계정병합 사용중지"></Label>
            </ListView.Header>
            <ListView.Footer>
                <Label Style="{DynamicResource labelStyle}" x:Name="lblFooter" Text="계정병합 추가하기"></Label>
            </ListView.Footer>
        </ListView>
    </ContentPage.Content>
</ContentPage>