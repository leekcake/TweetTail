        <Frame Margin="5" Padding="0" BackgroundColor="Gray">
            <forms:CachedImage WidthRequest="32" HeightRequest="32" x:Name="IconImage"></forms:CachedImage>
        </Frame>
    </ContentView.Content>
</ContentView>