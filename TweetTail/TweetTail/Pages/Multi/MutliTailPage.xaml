<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TweetTail.Pages.Multi.MutliTailPage">
    <ContentPage.Content>
        <StackLayout Orientation="Horizontal">
            <StackLayout x:Name="SideMenu" WidthRequest="40" HorizontalOptions="Fill"></StackLayout>
            <ScrollView Orientation="Horizontal">
                <StackLayout x:Name="TailContainer" Orientation="Horizontal">
                </StackLayout>
            </ScrollView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>